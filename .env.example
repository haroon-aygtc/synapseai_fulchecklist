# SynapseAI Production Environment Variables

# Database Configuration
DATABASE_URL="postgresql://username:password@localhost:5432/synapseai"
REDIS_URL="redis://localhost:6379"

# Authentication & Security
JWT_SECRET="your-super-secure-jwt-secret-key-here"
ENCRYPTION_KEY="your-32-character-encryption-key-here"
SESSION_SECRET="your-session-secret-key-here"

# AI Provider API Keys
OPENAI_API_KEY="sk-your-openai-api-key"
ANTHROPIC_API_KEY="sk-ant-your-anthropic-api-key"
GOOGLE_AI_API_KEY="your-google-ai-api-key"
MISTRAL_API_KEY="your-mistral-api-key"
GROQ_API_KEY="your-groq-api-key"

# Vector Database Configuration
VECTOR_DB_API_KEY="your-vector-db-api-key"
VECTOR_DB_ENVIRONMENT="production"
VECTOR_DB_INDEX="synapseai-knowledge"

# Email Configuration (for notifications)
SMTP_HOST="smtp.gmail.com"
SMTP_PORT="587"
SMTP_USER="your-email@gmail.com"
SMTP_PASS="your-app-password"

# File Storage Configuration
AWS_ACCESS_KEY_ID="your-aws-access-key"
AWS_SECRET_ACCESS_KEY="your-aws-secret-key"
AWS_REGION="us-east-1"
AWS_S3_BUCKET="synapseai-storage"

# Monitoring & Analytics
SENTRY_DSN="your-sentry-dsn"
ANALYTICS_API_KEY="your-analytics-api-key"

# Rate Limiting & Performance
RATE_LIMIT_WINDOW_MS="60000"
RATE_LIMIT_MAX_REQUESTS="1000"
MAX_CONCURRENT_EXECUTIONS="100"

# Browser Automation
BROWSER_POOL_SIZE="5"
BROWSER_TIMEOUT="30000"

# Webhook Configuration
WEBHOOK_SECRET="your-webhook-secret"

# API Configuration
NEXT_PUBLIC_API_URL="http://localhost:3001"

# App Configuration
NEXT_PUBLIC_APP_NAME="SynapseAI"
NEXT_PUBLIC_APP_DESCRIPTION="Enterprise AI Orchestration Platform"

# Environment
NODE_ENV="development"

# Analytics (Optional)
NEXT_PUBLIC_ANALYTICS_ID=""

# Feature Flags
NEXT_PUBLIC_ENABLE_2FA="true"
NEXT_PUBLIC_ENABLE_SSO="true"
NEXT_PUBLIC_ENABLE_ANALYTICS="true"